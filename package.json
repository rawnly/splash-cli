{
	"name": "splash-cli",
	"version": "3.4.11",
	"main": "build/extra/Unsplash.js",
	"readme": "README.md",
	"homepage": "https://splash-cli.app",
	"description": "Get beautiful wallpapers from unsplash.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/splash-cli/splash-cli"
	},
	"author": {
		"email": "fedevitale99@gmail.com",
		"name": "Federico Vitale",
		"url": "https://github.com/rawnly"
	},
	"scripts": {
		"build": "babel src -d build",
		"copyHTML": "rm -rf build/pages && cp -rf src/pages build",
		"start": "node build/bin/index.js",
		"start.norebuild": "node build/bin/index.js",
		"fix": "eslint src/**/*.js --color --fix",
		"test": "ava",
		"prebuild": "rm -rf build && npm run fix",
		"prestart": "npm run build",
		"postbuild": "npm run copyHTML",
		"prestartW": "npm run buildW",
		"pretest": "npm run build",
		"prepublish": "npm run fix"
	},
	"pkg": {
		"scripts": "build/**/*.js"
	},
	"bin": {
		"splash": "build/bin/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=6.11.4",
		"npm": ">=3.10.10"
	},
	"keywords": [
		"unsplash",
		"splash-cli",
		"splash",
		"wallpaper",
		"wallpapers",
		"unsplash-js",
		"photos",
		"photo",
		"photography",
		"baeutiful"
	],
	"dependencies": {
		"@scarf/scarf": "^1.0.5",
		"@sentry/node": "5.15.5",
		"@splash-cli/is-month": "^1.1.1",
		"@splash-cli/parse-unsplash-id": "^0.0.1",
		"@splash-cli/show-copy": "^1.1.0",
		"ava": "^1.4.1",
		"babel-polyfill": "^6.26.0",
		"chalk": "^2.4.1",
		"cli-table": "^0.3.1",
		"conf": "^1.4.0",
		"cross-env": "^6.0.3",
		"dotenv": "^6.2.0",
		"figures": "^2.0.0",
		"first-run": "^1.2.0",
		"fuzzy": "^0.1.3",
		"got": "^8.3.1",
		"image-size": "^0.7.4",
		"inquirer": "^5.2.0",
		"inquirer-autocomplete": "^0.12.2",
		"is-image": "^2.0.0",
		"isomorphic-fetch": "^2.2.1",
		"jsdom": "^11.10.0",
		"lodash": "^4.17.10",
		"meow": "^5.0.0",
		"mkdirp": "^0.5.1",
		"ms": "^2.1.1",
