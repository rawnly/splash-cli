{
	"name": "splash-cli",
	"version": "3.4.15",
	"main": "build/extra/Unsplash.js",
	"type": "commonjs",
	"readme": "README.md",
	"homepage": "https://splash-cli.app",
	"description": "Get beautiful wallpapers from unsplash.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/splash-cli/splash-cli"
	},
	"author": {
		"email": "fedevitale99@gmail.com",
		"name": "Federico Vitale",
		"url": "https://github.com/rawnly"
	},
	"scripts": {
		"build": "babel src -d build --source-maps",
		"copyHTML": "rm -rf build/pages && cp -rf src/pages build",
		"start": "node build/bin/index.js --enable-source-maps",
		"start.norebuild": "node build/bin/index.js",
		"fix": "eslint src/**/*.js --color --fix",
		"lint-staged": "lint-staged",
		"test": "jest",
		"prebuild": "rm -rf build && npm run fix",
		"prestart": "npm run build",
		"postbuild": "npm run copyHTML",
		"prepublish": "npm run fix"
	},
	"pkg": {
		"scripts": "build/**/*.js"
	},
	"bin": {
		"splash": "build/bin/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=6.11.4",
		"npm": ">=3.10.10"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		]
	},
	"keywords": [
		"unsplash",
		"splash-cli",
		"splash",
		"wallpaper",
		"wallpapers",
		"unsplash-js",
		"photos",
		"photo",
		"photography",
		"baeutiful"
	],
	"dependencies": {
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/runtime": "^7.12.1",
		"@sentry/node": "5.15.5",
		"@splash-cli/is-month": "^1.1.1",
		"@splash-cli/parse-unsplash-id": "^0.0.1",
		"@splash-cli/show-copy": "^1.1.0",
		"babel-polyfill": "^6.26.0",
		"chalk": "^2.4.1",
		"cli-table": "^0.3.1",
		"conf": "^1.4.0",
		"cross-env": "^6.0.3",
		"dotenv": "^6.2.0",
		"figures": "^2.0.0",
		"first-run": "^2.0.0",
		"fuzzy": "^0.1.3",
		"got": "^11.8.2",
		"image-size": "^0.7.4",
		"inquirer": "^5.2.0",
		"inquirer-autocomplete": "^0.12.2",
		"is-image": "^2.0.0",
		"isomorphic-fetch": "^3.0.0",
		"jest": "^26.6.3",
		"jsdom": "^11.10.0",
		"localtunnel": "^2.0.2",
		"lodash": "^4.17.10",
		"meow": "^8.0.0",
		"mkdirp": "^0.5.1",
		"ms": "^2.1.1",
		"normalize-url": "^2.0.1",
		"ora": "^2.1.0",
		"sharp": "^0.25.4",
		"simple-download": "^0.6.0",
		"terminal-link": "^1.1.0",
		"update-notifier": "^2.5.0",
		"wallpaper": "^4.2.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/plugin-transform-spread": "^7.2.2",
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"babel-eslint": "8",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-transform-inline-environment-variables": "^0.4.3",
		"cp-cli": "^2.0.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "4.x",
		"eslint-plugin-jest": "^24.4.2",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"rimraf": "^2.6.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
